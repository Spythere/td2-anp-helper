wjazd 1Wa 5 pt 1Pi * 1Wa_Wjazd_4,6 1Pi_Wyjazd_4,6 - -
wjazd 1Wa 5 - 1Pi * 1Wa_Wjazd_4,6 1Pi_Wyjazd_4,6 - -

